<div class="ticket-file[[+deleted]][[+new]] tcbillboard-file-meta" data-id="[[+id]]">
    <div>
        <a href="[[+url]]" class="ticket-file-link" title="[[+file]]" target="_blank">
            <div class="ticket-file-image-wrapper tcbillboard-file-wrapper">
                <img src="[[+thumb]]" class="ticket-file-image"/>
            </div>
        </a>
        <div class="ticket-file-template">
            <a href="[[+url]]" title="[[+name]]">
                <img src="[[+thumb]]"/>
            </a>
        </div>
    </div>

    <div class="ticket-file-meta">
        <a href="#" class="tcbillboard-file-btn btn btn-default ticket-file-delete" data-file="img/delete">[[%ticket_file_delete]]</a>
        <a href="#" class="tcbillboard-file-btn btn btn-default ticket-file-restore" data-file="img/restore">[[%ticket_file_restore]]</a>
    </div>
</div>
<!--tickets_deleted  deleted-->
<!--tickets_new  new-->